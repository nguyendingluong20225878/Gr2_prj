{
  "name": "@gr2/shared",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "db:seed": "tsx --env-file=.env src/db/seed.ts",
    "test:connection": "tsx --env-file=.env scripts/test-connection.ts",
    "test:schemas": "tsx --env-file=.env scripts/test-schemas.ts",
    "test:integrity": "tsx --env-file=.env scripts/test-data-integrity.ts",
    "test:all": "npm run test:connection && npm run test:schemas && npm run test:integrity"
  },
  "dependencies": {
    "@types/web-push": "^3.6.4",
    "mongoose": "^8.8.0",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "tsx": "^4.7.0"
  }
}
